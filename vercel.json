{
    "redirects": [
        {
            "source": "/users",
            "destination": "/api/users",
            "permanent": true
        },
        {
            "source": "/users/:path*",
            "destination": "/api/users/:path*",
            "permanent": true
        },
        {
            "source": "/t",
            "destination": "/api/t",
            "permanent": true
        },
        {
            "source": "/t/:path*",
            "destination": "/api/t/:path*",
            "permanent": true
        }
    ],
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/:path*"
        }
    ]
}